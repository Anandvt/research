

fn main
    set %a "touch.test"
    echo "touching file (%[a]s)\n"
    fs.stat %a
    cmp 0 1
    jnz +2
      perror "stat %[a]s"
      halt

    fs.touch %a
    jz +2
      perror "unable to create %[a]s"
      halt

    fs.chown %a 1000
    fs.chgrp %a 1001
    fs.chmod %a 0647

    echo "done!\n"
    ret
